confidence.interval <- function(mean,z,sd,n){
LL <- mean - z * (sd/sqrt(n))
UL <- mean + z * (sd/sqrt(n))
return(LL)
return(UL)
}
confidence.interval(mean = 18.05, z = 1.645, sd = 5, n = 20)
